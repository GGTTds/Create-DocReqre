        mc:Ignorable="d"
        Title="CreateOtcEx" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="12*"/>
            <RowDefinition Height="80*"
                           />
            <RowDefinition Height="13*"/>
