        <TextBox x:Name="Zavod" Width="430" Height="30" Margin="318,10,0,280" Grid.Row="1" HorizontalAlignment="Left"></TextBox>
        <Label Name="RR" Height="30" Content="Завод:" Grid.Row="1" Margin="265,10,0,280" HorizontalAlignment="Left" Width="48"/>
        <Label Name="TT" Content=" Выберите нужную продукцию" Height="33" FontSize="16" Margin="278,10,267,5" />
        <CheckBox x:Name="BP114f" Content="Закладная под ключ ВР 1 1/4F"  HorizontalAlignment="Left" Margin="57,85,0,0" Grid.Row="1" VerticalAlignment="Top" Width="195"/>
